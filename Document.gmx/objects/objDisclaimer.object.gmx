draw_set_font(fntDisclaimer)
draw_set_valign(fa_center)
draw_set_halign(fa_center)
draw_set_color(global.albedo)
draw_sprite_ext(sprPreface, 0, display_get_gui_width() / 2, display_get_gui_height() / 2, 1, 1, 0, c_white, 1)
